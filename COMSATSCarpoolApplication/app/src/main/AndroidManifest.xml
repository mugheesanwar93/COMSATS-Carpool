<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mugheesanwar.comsatscarpoolapplication" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:name=".ApplicationController"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".Testing"
            android:label="@string/title_activity_testing" >
        </activity>
        <activity
            android:name=".DesignationSelect"
            android:label="@string/title_activity_designation_select" >
        </activity>
        <activity
            android:name=".OptionSelect"
            android:label="@string/title_activity_option_select" >
        </activity>
        <activity
            android:name=".Form"
            android:label="@string/title_activity_form" >
        </activity>
    </application>

</manifest>
